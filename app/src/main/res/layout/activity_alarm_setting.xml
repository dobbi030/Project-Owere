<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".user.activity.main.mypageActivity.NoticeActivity">

    <ImageView
        android:id="@+id/alarmsetting_goback_button"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/black"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="알림설정"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/alarmsetting_line1"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="54dp"
        android:background="#C4C4C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="69dp"
        android:layout_height="24dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="알림 및 소리"
        android:textColor="@color/black"
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarmsetting_line1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="31dp"
        android:text="채팅 메시지 알림"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarmsetting_line1" />

    <Switch
        android:id="@+id/alarmsetting_chatting_onoff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginRight="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line1" />

    <View
        android:id="@+id/alarmsetting_line2"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="9dp"
        android:background="#C4C4C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_chatting_onoff"/>

    <TextView
        android:id="@+id/alarmsetting_header_alarm"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:text="알림음"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line2"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="5dp"
        />

    <TextView
        android:layout_marginTop="5dp"
        android:layout_marginRight="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="14sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="오~월"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line2"
        />

    <View
        android:id="@+id/alarmsetting_line3"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="9dp"
        android:background="#C4C4C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_header_alarm"/>

    <TextView
        android:layout_width="69dp"
        android:layout_height="24dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="서비스 알림"
        android:textColor="@color/black"
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarmsetting_line3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="31dp"
        android:text="이벤트 알림"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarmsetting_line3" />

    <Switch
        android:id="@+id/alarmsetting_event_onoff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginRight="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line3" />

    <View
        android:id="@+id/alarmsetting_line4"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="9dp"
        android:background="#C4C4C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_event_onoff"/>


    <TextView
        android:id="@+id/alarmsetting_coupon_publish_alarm"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:text="쿠폰 발행 알림"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line4"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="5dp"
        />
    <Switch
        android:id="@+id/alarmsetting_coupon_publish_onoff"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginTop="9dp"
        android:layout_marginRight="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line4" />


    <View
        android:id="@+id/alarmsetting_line5"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="9dp"
        android:background="#C4C4C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_coupon_publish_onoff"/>


    <TextView
        android:id="@+id/alarmsetting_coupon_expiration_alarm"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:text="쿠폰 만료 알림"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line5"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="5dp"
        />
    <Switch
        android:id="@+id/alarmsetting_coupon_expiration_onoff"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginTop="9dp"
        android:layout_marginRight="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line5" />




    <View
        android:id="@+id/alarmsetting_line6"
        android:layout_width="0dp"
        android:layout_height="4dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="9dp"
        android:background="#C4C4C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_coupon_expiration_onoff"/>


    <TextView
        android:layout_width="69dp"
        android:layout_height="24dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="기타"
        android:textColor="@color/black"
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarmsetting_line6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="31dp"
        android:text="진동"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarmsetting_line6" />

    <Switch
        android:id="@+id/alarmsetting_vibration_onoff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginRight="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/alarmsetting_line6" />


</androidx.constraintlayout.widget.ConstraintLayout>