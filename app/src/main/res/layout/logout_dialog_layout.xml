<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/text_logout_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="로그아웃 하시겠습니까?"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="16sp"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/logout_no"
        android:background="@drawable/rounded_black_square_background"
        android:gravity="center"
        android:layout_marginTop="30dp"
        android:layout_width="137dp"
        android:layout_height="44dp"
        android:textColor="@color/black"
        android:text="아니오"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/text_logout_dialog"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/logout_yes"
        />
    <TextView
        android:id="@+id/logout_yes"
        android:background="@drawable/rounded_black_square_background"
        android:gravity="center"

        android:layout_width="137dp"
        android:layout_height="44dp"
        android:textColor="@color/black"
        android:text="네"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/logout_no"
        app:layout_constraintTop_toTopOf="@+id/logout_no"
        app:layout_constraintBottom_toBottomOf="@+id/logout_no"
        app:layout_constraintEnd_toEndOf="parent"
        />



</androidx.constraintlayout.widget.ConstraintLayout>