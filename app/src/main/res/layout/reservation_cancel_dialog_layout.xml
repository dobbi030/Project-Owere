<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/text_reservation_cancel_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="정말 예약을 취소하실건가요?"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="16sp"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/reservation_cancel_yes"
        android:background="@drawable/rounded_black_square_background"
        android:gravity="center"
        android:layout_marginTop="30dp"
        android:layout_width="137dp"
        android:layout_height="44dp"
        android:textColor="@color/black"
        android:text="네. 취소할게요"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@+id/text_reservation_cancel_dialog"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/reservation_cancel_no"
        />
    <TextView
        android:id="@+id/reservation_cancel_no"
        android:background="@drawable/rounded_black_square_background"
        android:gravity="center"

        android:layout_width="137dp"
        android:layout_height="44dp"
        android:textColor="@color/black"
        android:text="아니요"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/reservation_cancel_yes"
        app:layout_constraintTop_toTopOf="@+id/reservation_cancel_yes"
        app:layout_constraintBottom_toBottomOf="@+id/reservation_cancel_yes"
        app:layout_constraintEnd_toEndOf="parent"
        />



</androidx.constraintlayout.widget.ConstraintLayout>